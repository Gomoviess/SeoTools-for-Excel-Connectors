    <![CDATA[
		DateTime AdjustDate() {
			double n = Model.Inp;
			DateTime output = (DateTime)Model.DateInterval.StartDate;
			if(Model.Granularity == "DAY")
				output = output.AddDays(n);
			else if(Model.Granularity == "HOUR")
				output = output.AddHours(n);
			return output;
		}
    ]]>
