<?xml version="1.0" encoding="utf-8" ?>
<Suite Title="Twitter Ads" Id="TwitterAds" Category="Social" RequireVersion="9.5.4" LoginButton="https://seotoolsforexcel.com/logins/twitter.png" SourceUrl="https://github.com/nielsbosma/SeoTools-for-Excel-Connectors/blob/master/TwitterAds.xml" HelpUrl="http://seotoolsforexcel.com/twitter-ads/" HelpText="Documentation">

  <Author Name="Victor Sandberg" Url="http://community.seotoolsforexcel.com/users/diskborste/activity"/>

	<SeoToolsAuthenticator StayAuthenticated="true" Key="twitter-ads"/>

  <Resources>
    <Resource Id="Params">
      <Select Id="Granularity" Title="Granularity" DefaultValue="NONE">
        <DataSource>
          <Item Id="TOTAL" Title="None"/>
          <Item Id="DAY" Title="Daily"/>
          <Item Id="HOUR" Title="Hourly"/>´
        </DataSource>
      </Select>
      <Select Id="Placement" Title="Placement" DefaultValue="ALL_ON_TWITTER">
        <DataSource>
          <Item Id="ALL_ON_TWITTER" Title="All on Twitter"/>
          <Item Id="PUBLISHER_NETWORK" Title="Publisher Network"/>
        </DataSource>
      </Select>
      <DateInterval Id="DateInterval" Title="Date Interval"/>
		</Resource>
		<Resource Id="PrepareZip">
			<Prepare>
				<Connector Id="GetZipUrl" Parameters="AccountId,EntityType,EntityId,Granularity,Placement,DateInterval,MetricType"/>
			</Prepare>
		</Resource>
    <Resource Id="FetchZip">
			<Fetch>
				<HttpSettings>
					<RequestContentType>gzip</RequestContentType>
				</HttpSettings>
				<Fetch.Url>
					<![CDATA[
						@(Model.GZipUrl)
					]]>
				</Fetch.Url>
			</Fetch>
		</Resource>
    <Resource Id="HttpSettings">
      <HttpSettings>
        <OAuth>
          <ConsumerKey>@Model.OAuthConsumerKey</ConsumerKey>
          <ConsumerSecret>@Model.OAuthConsumerSecret</ConsumerSecret>
          <Token>@Model.OAuthToken</Token>
          <TokenSecret>@Model.OAuthTokenSecret</TokenSecret>
        </OAuth>
      </HttpSettings>
		</Resource>
    <Resource Id="Fail">
		  <Fail>
				<JsonPath Expr="errors[0].message"/>
				<JsonPath Expr="error"/>
			</Fail>
		</Resource>
  </Resources>

  <RestConnector Id="Accounts" Hidden="true">
    <Fetch>
			<Resource Id="HttpSettings"/>
      <Fetch.Url>
        <![CDATA[
        https://ads-api.twitter.com/8/accounts
        ]]>
      </Fetch.Url>
    </Fetch>
    <Parse>
      <JsonPath Expr="$.data[*]">
        <JsonPath Expr="id" Id="Id" Title="Account Id"/>
        <JsonPath Expr="name" Id="Name" Title="Account Name"/>
      </JsonPath>
    </Parse>
    <Resource Id="Fail"/>
  </RestConnector>

