  <RestConnector Id="ListTweets" Title="Tweets" Group="List Data">
    <Parameters>
      <Text Id="AccountId" Title="Account Id" Debug.DefaultValue="18ce54a6tlx" Required="true" Select.Connector="Accounts"/>
      <Select Id="TweetType" Title="Tweet Type" DefaultValue="PUBLISHED">
        <DataSource>
					<Item Id="PUBLISHED" Title="Published"/>
					<Item Id="SCHEDULED" Title="Scheduled"/>
					<Item Id="DRAFT" Title="Draft"/>
        </DataSource>
      </Select>
			<Select Id="TimelineType" Title="Timeline Type" DefaultValue="ALL">
        <DataSource>
					<Item Id="ALL" Title="All"/>
					<Item Id="NULLCAST" Title="Promoted-only"/>
					<Item Id="ORGANIC" Title="Organic"/>
        </DataSource>
      </Select>
			<Checkbox Id="IncludeMentionsReplies" Title="Include Mentions &amp; Replies" DefaultValue="true"/>
    </Parameters>
    <Paging PageSize="200" EvenPages="false">
      <Parse>
        <JsonPath Id="Cursor" Expr="next_cursor"/>
      </Parse>
    </Paging>
    <Fetch>
			<Resource Id="HttpSettings"/>
      <Fetch.Url>
        <![CDATA[
        https://ads-api.twitter.com/8/accounts/@(Model.AccountId)/tweets
				?tweet_type=@(Model.TweetType)
				&timeline_type=@(Model.TimelineType)
				&count=@(Model.PageCursor.NextTake)
				&include_mentions_and_replies=@(Model.IncludeMentionsReplies)
				@(Model.PageCursor.Page != 0 ? "&cursor=" + Model.Cursor : "")
        ]]>
      </Fetch.Url>
    </Fetch>
    <Parse>
      <JsonPath Expr="$.data[*]">
				<Compute Expr="https://www.twitter.com/@(Model.Inp1)/statuses/@(Model.Inp2)/" Id="Url" Title="URL">
					<JsonPath Expr="user.screen_name" Id="Inp1" DefaultValue=""/>
					<JsonPath Expr="id_str" Id="Inp2" DefaultValue=""/>
				</Compute>
				<JsonPath Expr="id_str" Id="Id"/>
				<JsonPath Expr="created_at" Id="CreatedAt" Title="Date" Converter="DateTime" Converter.SourceFormat="ddd MMM dd HH:mm:ss +ffff yyyy"/>
				<JsonPath Expr="full_text" Id="Text" DefaultValue=""/>
				<JsonPath Expr="retweet_count" Id="Retweets" Converter="Int"/>
				<JsonPath Expr="favorite_count" Id="Likes" Converter="Int"/>
				<JsonPath Expr="tweet_type" Id="TweetType" Title="Tweet Type"/>
				<JsonPath Expr="nullcast" Id="PromotedOnly" Title="Promoted-only"/>
				<JsonPath Expr="csv(entities.hashtags[*].text)" Id="HashtagEntities" Title="Hashtag Entities" Converter="Auto" DefaultValue=""/>
				<JsonPath Expr="csv(entities.urls[*].url)" Id="UrlEntities" Title="URL Entities" Converter="String" DefaultValue=""/>
				<JsonPath Expr="csv(entities.user_mentions[*].screen_name)" Id="MentionsEntities" Title="User Mentions Entities" Converter="Auto" DefaultValue=""/>
				<JsonPath Expr="entities.media[0].media_url" Id="PhotoUrl" Title="Media URL" Converter="String" DefaultValue=""/>
				<Compute Expr="@Utils.StripTags(Model.Inp)" Id="Source">
					<JsonPath Expr="source" Id="Inp" DefaultValue=""/>
				</Compute>
				<JsonPath Expr="user.screen_name" Id="Username"/>
				<Compute Expr="https://www.twitter.com/@(Model.Inp)/" Id="UserUrl" Title="User URL">
					<JsonPath Expr="user.screen_name" Id="Inp" DefaultValue=""/>
				</Compute>
      </JsonPath>
    </Parse>
    <Resource Id="Fail"/>
  </RestConnector>

