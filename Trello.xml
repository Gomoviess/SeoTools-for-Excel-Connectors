        ]]>
      </Fetch.Url>
    </Fetch>
    <Parse>
      <JsonPath Expr="$[*]">
        <JsonPath Expr="id" Id="Id" Title="Board Id"/>
        <JsonPath Expr="name" Id="Name" Title="Board Name"/>
      </JsonPath>
    </Parse>
    <Resource Id="Fail"/>
  </RestConnector>

